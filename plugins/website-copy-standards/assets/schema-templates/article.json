{
  "@context": "https://schema.org",
  "@type": "Article",
  "headline": "{{HEADLINE}}",
  "name": "{{NAME}}",
  "description": "{{DESCRIPTION}}",
  "url": "{{URL}}",
  "image": "{{IMAGE_URL}}",
  "datePublished": "{{DATE_PUBLISHED}}",
  "dateModified": "{{DATE_MODIFIED}}",
  "author": {
    "@type": "Person",
    "name": "{{AUTHOR_NAME}}",
    "url": "{{AUTHOR_URL}}"
  },
  "publisher": {
    "@type": "Organization",
    "name": "{{PUBLISHER_NAME}}",
    "logo": {
      "@type": "ImageObject",
      "url": "{{PUBLISHER_LOGO_URL}}"
    }
  },
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "{{URL}}"
  }
}
